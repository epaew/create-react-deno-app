{
  "compilerOptions": {
    "lib": [
      "dom",
      "esnext"
    ]
  },
  "importMap": "import_map.json",
  "tasks": {
    "build": "deno run --allow-env --allow-net --allow-read --allow-run --allow-write bin/build.ts"
    "npm:add": "deno run -A https://esm.sh/v99 add",
